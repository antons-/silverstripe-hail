{
    "name": "firebrandhq/silverstripe-hail",
    "description": "Silverstripe 4 plugin to integrate with Hail (https://get.hail.to)",
    "type": "silverstripe-module",
    "keywords": ["silverstripe", "News", "Hail", "Content"],
    "license": "MIT",
    "authors": [{
        "name": "Firebrand Limited",
        "email": "developers@firebrand.nz"
    }],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Firebrand\\Hail\\": "code/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
